{
  "name": "newsbot-slack",
  "version": "1.0.0",
  "description": "Slack bot that delivers daily retail innovation news summaries",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "node src/index.js --test",
    "generate-keywords": "node scripts/generate-keywords.js",
    "generate-keywords:dry-run": "node scripts/generate-keywords.js --dry-run",
    "generate-keywords:verbose": "node scripts/generate-keywords.js --verbose",
    "validate-keywords": "node scripts/validate-keywords.js",
    "prestart": "npm run generate-keywords"
  },
  "keywords": [
    "slack",
    "bot",
    "news",
    "retail",
    "innovation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.6.5",
    "axios-retry": "^4.5.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "p-retry": "^7.1.0",
    "rss-parser": "^3.13.0",
    "string-similarity": "^4.0.4"
  }
}
